        "2": {
            "ID": "1853821",
            "URL": "https://thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/seasons/official/2",
            "TitleEnglish": null,
            "SummaryEnglish": null,
            "# Episodes": 12,
            "Episodes": {
                "1": {
                    "ID": "7680144",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7680144",
                    "TitleEnglish": "Enter Goryo",
                    "SummaryEnglish": "Two of Nana's classmates play a Ouija-style game after school and find themselves in supernatural trouble. The head of a competing agency shows up to deal with the problem, but has another objective in mind involving Muhyo & Roji's bureau.",
                    "Aliases": []
                },
                "2": {
                    "ID": "7830131",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830131",
                    "TitleEnglish": "The Ruins of Akagawa Estates",
                    "SummaryEnglish": "Goryo challenges Muhyo to a winner-take-all contest over who can exorcise the spirits of the long abandoned Akagawa Estates housing project. Nana learns the tragic tale that led to the site being haunted. Ebisu finds himself in a tight spot.",
                    "Aliases": []
                },
                "3": {
                    "ID": "7830134",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830134",
                    "TitleEnglish": "Win or Lose",
                    "SummaryEnglish": "Nana has important information for Muhyo and Roji about the doll spirits. Muhyo coaches Nana about a technique she must perform, something he has never done for Roji. As the contest reaches its conclusion, Muhyo makes a surprise announcement.",
                    "Aliases": []
                },
                "4": {
                    "ID": "7830135",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830135",
                    "TitleEnglish": "Encounter",
                    "SummaryEnglish": "After Roji is placed on suspension by Muhyo, he is sent to the Association with Imai to try to sort things out for himself. Dejected about not being able to join in with helping Rio, Roji recounts for Imai the first time he ever met Muhyo.",
                    "Aliases": []
                },
                "5": {
                    "ID": "7830137",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830137",
                    "TitleEnglish": "The Testing Begins",
                    "SummaryEnglish": "With a little guidance from Imai, Roji decides to undergo the exams being held by the Magic Law Academy to help him better understand himself, and in the process, makes some new friends. Muhyo lays the groundwork for an ambitions new contract.",
                    "Aliases": []
                },
                "6": {
                    "ID": "7830138",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830138",
                    "TitleEnglish": "Their Respective Trials",
                    "SummaryEnglish": "A vengeful spirit stalks the enrichment camp participants, while Roji, Lili and Maril enlist Ebisu's assistance. Muhyo takes the extraordinary measure of soliciting a contract with Hades Ruararie, but he must first prove his strength through combat.",
                    "Aliases": []
                },
                "7": {
                    "ID": "7830139",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830139",
                    "TitleEnglish": "Trust",
                    "SummaryEnglish": "Roji and his companions try to evade Bellocent. With all other options exhausted, Maril proposes a risky means of defeating the creature. Muhyo continues his arduous negotiations with Hades. Ebisu recounts the details of his troubled past.",
                    "Aliases": []
                },
                "8": {
                    "ID": "7830968",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830968",
                    "TitleEnglish": "Conclusion",
                    "SummaryEnglish": "Roji makes one last push to incapacitate Bellocent. Muhyo reminds Yoichi what they are fighting for, and manages to win over Hades. Page offers Roji a position working for him, prompting Roji to reflect on what he has learned during his suspension.",
                    "Aliases": []
                },
                "9": {
                    "ID": "7830970",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830970",
                    "TitleEnglish": "Under the Sod",
                    "SummaryEnglish": "A panicked call from Kenji brings Muhyo, Roji and Nana back to the office, where Kenji says he saw a spirit. When Muhyo falls asleep and cannot be awakened, Roji steps up to take matters into his own hands, but is he ready to act alone?",
                    "Aliases": []
                },
                "10": {
                    "ID": "7830972",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7830972",
                    "TitleEnglish": "Worms",
                    "SummaryEnglish": "Tomas challenges Goryo, while Sakon is dismissed to contact Ebisu, who asks Muhyo and the others for their help. The group heads to Tomas's hideout, where they must overcome many obstacles if they are going to rescue Goryo in time.",
                    "Aliases": []
                },
                "11": {
                    "ID": "7926306",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7926306",
                    "TitleEnglish": "Beelzebub's Armor",
                    "SummaryEnglish": "Following an unsettling development with the Association's leadership, Page enlists Imai's help in planning his next move. Muhyo and the others continue to contend against Tomas, who continues to pose a challenge despite their best efforts.",
                    "Aliases": []
                },
                "12": {
                    "ID": "7937731",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/muhyo-and-rojis-bureau-of-supernatural-investigation/episodes/7937731",
                    "TitleEnglish": "That Which is Perfect",
                    "SummaryEnglish": "Tomas prepares a display of \"perfect despair\" by opening up a Styxsand vortex that leads to Hell, placing Muhyo and the others in great danger. In order to escape, it falls to Roji to locate Tomas's Forbidden Magic Law book, the source of his power.",
                    "Aliases": []
                }
            }
        }
    }
