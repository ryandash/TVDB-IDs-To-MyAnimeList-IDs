            "# Episodes": 24,
            "Episodes": {
                "1": {
                    "ID": "9620307",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9620307",
                    "TitleEnglish": "Maomao",
                    "SummaryEnglish": "After being kidnapped and forced to work as a servant in the rear palace, former apothecary Maomao inadvertently draws the attention of the higher ranks when she investigates what's causing the emperor's infant heirs to fall mysteriously ill.",
                    "Aliases": []
                },
                "2": {
                    "ID": "9973394",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973394",
                    "TitleEnglish": "Chilly Apothecary",
                    "SummaryEnglish": "Maomao tries to settle into her promotion to lady-in-waiting and food taster, but struggles to fend off both boredom and the overly-familiar Jinshi. That is, until she gets an assignment of her dreams: to concoct an aphrodisiac.",
                    "Aliases": []
                },
                "3": {
                    "ID": "9973395",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973395",
                    "TitleEnglish": "The Unsettling Matter of the Spirit",
                    "SummaryEnglish": "Tasked with investigating a ghostly woman dancing nightly on the palace walls, Maomao finds a sleepwalking Concubine Fuyou. But with no medicinal cure for sleepwalking, she's forced to dig deeper to find the real reason behind Fuyou's affliction.",
                    "Aliases": []
                },
                "4": {
                    "ID": "9973396",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973396",
                    "TitleEnglish": "The Threat",
                    "SummaryEnglish": "Under direct orders from the emperor himself, Maomao tries to care for an ailing Concubine Lihua, but finds her work impeded by Lihua's own ladies-in-waiting.",
                    "Aliases": []
                },
                "5": {
                    "ID": "9973397",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973397",
                    "TitleEnglish": "Covert Operations",
                    "SummaryEnglish": "A eunuch stricken by a curse comes begging for aid. Meanwhile, the rear palace buzzes with excitement as everyone prepares for the upcoming garden party, and Gyokuyou's ladies-in-waiting aren't about to let Maomao get away without dressing her up.",
                    "Aliases": []
                },
                "6": {
                    "ID": "9973398",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973398",
                    "TitleEnglish": "The Garden Party",
                    "SummaryEnglish": "Maomao does her best to navigate the complex social situations at the garden party, including meeting the other Upper First rank concubines. But once she settles down to do her work as a food-taster, she discovers a surprise in a bowl of soup.",
                    "Aliases": []
                },
                "7": {
                    "ID": "9973399",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973399",
                    "TitleEnglish": "Homecoming",
                    "SummaryEnglish": "Gaoshun goes to Maomao to get her help in resolving the events of the garden party. Later, Xiaolan explains the purpose of the hairpins Maomao was gifted, and Maomao makes a deal with Lihaku in order to finally visit home.",
                    "Aliases": []
                },
                "8": {
                    "ID": "9973400",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973400",
                    "TitleEnglish": "Wheat Stalks",
                    "SummaryEnglish": "Maomao's peaceful morning at home is interrupted when she's called in to investigate an apparent double suicide at one of the local brothels.",
                    "Aliases": []
                },
                "9": {
                    "ID": "9973401",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973401",
                    "TitleEnglish": "Suicide or Murder?",
                    "SummaryEnglish": "An older acquaintance of Jinshi's dies while drinking at a party. A servant's body turns up in a moat. Both deaths are written off as self-inflicted, but Maomao's keen observations tell her that there's more to both stories.",
                    "Aliases": []
                },
                "10": {
                    "ID": "9973402",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973402",
                    "TitleEnglish": "Honey",
                    "SummaryEnglish": "A tea party hosted by Lady Gyokuyou puts the politics of the rear palace at the forefront, and Maomao learns something new about Lady Lishu. His true intentions unclear, Jinshi sends Maomao to work for Lady Ah-Duo.",
                    "Aliases": []
                },
                "11": {
                    "ID": "9973403",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973403",
                    "TitleEnglish": "Reducing Two to One",
                    "SummaryEnglish": "Maomao confronts Fengming about the dark secret she's been hiding from her noble Lady Ah-Duo.",
                    "Aliases": []
                },
                "12": {
                    "ID": "9973404",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973404",
                    "TitleEnglish": "The Eunuch and the Courtesan",
                    "SummaryEnglish": "Discovering that Maomao's kidnappers were connected to Fengming's family, Jinshi is faced with a choice between keeping Maomao in the rear palace where he can be near her or releasing her from her service.",
                    "Aliases": []
                },
                "13": {
                    "ID": "9973405",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/9973405",
                    "TitleEnglish": "Serving in the Outer Court",
                    "SummaryEnglish": "To Maomao's surprise, Jinshi brings her directly to his house instead of taking her back to the rear palace, and tells her she's to work in the outer court. Not only is there much to learn, it seems there's many eyes on her as the new hire.",
                    "Aliases": []
                },
                "14": {
                    "ID": "10102917",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102917",
                    "TitleEnglish": "The New Pure Consort",
                    "SummaryEnglish": "Ah-Duo's replacement has arrived in the rear palace, and Maomao is assigned to give a certain lecture to all four high concubines. Later, Maomao helps Lihaku investigate a warehouse explosion.",
                    "Aliases": []
                },
                "15": {
                    "ID": "10102918",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102918",
                    "TitleEnglish": "Raw Fish",
                    "SummaryEnglish": "Gaoshun requests Maomao's help in solving an apparent poisoning case. Meanwhile, Jinshi deals with an unwelcome visitor who's taken to lingering in his office.",
                    "Aliases": []
                },
                "16": {
                    "ID": "10102919",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102919",
                    "TitleEnglish": "Lead",
                    "SummaryEnglish": "A shack, a chest of drawers, and a fishbowl are all that a talented metalworker leaves to his three sons upon his death. Maomao is assigned to investigate how these strange inheritances all fit together.",
                    "Aliases": []
                },
                "17": {
                    "ID": "10102920",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102920",
                    "TitleEnglish": "A Jaunt Around Town",
                    "SummaryEnglish": "Covering up Jinshi's otherworldly beauty is no easy task, but after Maomao takes the challenge head-on, she's surprised when she's also asked to don the disguise of a noblewoman and escort \"Jinka\" outside the palace.",
                    "Aliases": []
                },
                "18": {
                    "ID": "10102921",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102921",
                    "TitleEnglish": "Lakan",
                    "SummaryEnglish": "Back at home, Maomao visits an annex building at the Verdigris House that hides one of the brothel's secrets. Lakan continues to antagonize Jinshi as his connection to Maomao becomes clear.",
                    "Aliases": []
                },
                "19": {
                    "ID": "10102922",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102922",
                    "TitleEnglish": "Chance or Something More",
                    "SummaryEnglish": "A discussion with Lihaku about recent events leads Maomao to realize that the mysteries she's helped to solve are connected. As the final piece of the puzzle falls into place, she rushes to stop impending disaster.",
                    "Aliases": []
                },
                "20": {
                    "ID": "10102923",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102923",
                    "TitleEnglish": "Thornapple",
                    "SummaryEnglish": "Maomao was able to stop the assassination attempt, but was heavily injured in the process. However, the injuries aren't enough to stop her curiosity, driving her to confront the culprit behind the incident.",
                    "Aliases": []
                },
                "21": {
                    "ID": "10102924",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/kusuriya-no-hitorigoto/episodes/10102924",
                    "TitleEnglish": "How to Buy Out a Contract",
                    "SummaryEnglish": "Back to work at the rear palace, Maomao listens to Guen talk about his family's business. Meanwhile, Lihaku hears a rumor that whips him into action.",
                    "Aliases": []
                },
                "22": {
