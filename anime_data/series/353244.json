            "# Episodes": 24,
            "Episodes": {
                "1": {
                    "ID": "6838343",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6838343",
                    "Titles": {
                        "eng": "Episode 1",
                        "jpn": "第1羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "京都に住むコウテイペンギンのひな、ちとせちゃん。\r\n今日は嵐山に来ています。京都の人たちにはすっかりおなじみで、道行くみんなが声を掛けてくれるのですが…。"
                    },
                    "Aliases": []
                },
                "2": {
                    "ID": "6860441",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6860441",
                    "Titles": {
                        "eng": "Episode 2",
                        "jpn": "第2羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "電車で伏見稲荷大社にやってきたちとせちゃん。鳥居をぺたぺたと進んでいきますが、突然分かれ道が現れてびっくり！しかしその横を、女性2人が通り過ぎていき…？"
                    },
                    "Aliases": []
                },
                "3": {
                    "ID": "6868913",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6868913",
                    "Titles": {
                        "eng": "Episode 3",
                        "jpn": "第3羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "夜の街を歩くちとせちゃん。にぎやかな先斗町にたどり着くと、そこには笑い声と湯気がもれる楽しげなおでんの屋台がありました。"
                    },
                    "Aliases": []
                },
                "4": {
                    "ID": "6869052",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6869052",
                    "Titles": {
                        "eng": "Episode 4",
                        "jpn": "第4羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "哲学の道にやってきたちとせちゃん。長い一本道を見て思わず走り出してしまいます。するとそこに、横から突然カエルが参戦してきて…？"
                    },
                    "Aliases": []
                },
                "5": {
                    "ID": "6880095",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6880095",
                    "Titles": {
                        "eng": "Episode 5",
                        "jpn": "第5羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "道中安全の神「ヤタガラス」について教えてもらったちとせちゃん。風呂敷にステッカーを付けて下鴨神社を散策するのですが…。"
                    },
                    "Aliases": []
                },
                "6": {
                    "ID": "6906405",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6906405",
                    "Titles": {
                        "eng": "Episode6",
                        "jpn": "第6羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "鴨川沿いを歩くちとせちゃん。どうやらここはカップルが集まる場所のようです。構ってほしいちとせちゃんですが、なかなかみんなに相手にされなくて…？"
                    },
                    "Aliases": []
                },
                "7": {
                    "ID": "6906410",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6906410",
                    "Titles": {
                        "eng": "Episode7",
                        "jpn": "第7羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "素敵な舞妓さんに出会ったちとせちゃん。ガラスに映る自分を見て何だか落ち込んでいる様子。そんな時、音羽の滝の水を飲めば美人になれるという話を聞いて…？"
                    },
                    "Aliases": []
                },
                "8": {
                    "ID": "6923554",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6923554",
                    "Titles": {
                        "eng": "Episode8",
                        "jpn": "第8羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "通りすがりの親子をじっと見つめるちとせちゃん。どうやら母ペンギンを思い出したようです。そんな親子を追いかけてみると、ちとせちゃんは銀閣寺にたどり着いて…。"
                    },
                    "Aliases": []
                },
                "9": {
                    "ID": "6923559",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6923559",
                    "Titles": {
                        "eng": "Episode9",
                        "jpn": "第9羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "建仁寺にやってきたちとせちゃん。風神の真似をする男の子を見て、ちとせちゃんも思わず真似をします。するとそこに、本物の風神雷神が現れて…。"
                    },
                    "Aliases": []
                },
                "10": {
                    "ID": "6930145",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6930145",
                    "Titles": {
                        "eng": "Episode10",
                        "jpn": "第10羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ちとせちゃん、京都水族館にやってきました。ここにはペンギンの仲間もたくさんいます。そこでちとせちゃん、ペンギンエリアの中に侵入を試みるのですが…？"
                    },
                    "Aliases": []
                },
                "11": {
                    "ID": "6948347",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6948347",
                    "Titles": {
                        "eng": "Episode11",
                        "jpn": "第11羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "今日は不機嫌そうなちとせちゃん。何だかうまくいかないことがあったようです。そんなちとせちゃんがたどり着いたのは、高い高い京都タワーでした。"
                    },
                    "Aliases": []
                },
                "12": {
                    "ID": "6948349",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6948349",
                    "Titles": {
                        "eng": "Episode12",
                        "jpn": "第12羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "買い物を頼まれたちとせちゃん、師走でにぎわう錦市場にやってきました。メモに沿って買い物を進めていくのですが、あれ、メモがなくなった…！？"
                    },
                    "Aliases": []
                },
                "13": {
                    "ID": "6948353",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6948353",
                    "Titles": {
                        "eng": "Episode13",
                        "jpn": "第13羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "お友達の橘千尋に声をかけられたちとせちゃん。美術部員である彼女の絵のモデルとして、一緒に南禅寺へとやってきましたが、どうやら彼女には悩みがあるようで…？"
                    },
                    "Aliases": []
                },
                "14": {
                    "ID": "6987476",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/6987476",
                    "Titles": {
                        "eng": "Episode14",
                        "jpn": "第14羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "いつも身に付けている風呂敷に穴が空いてしまったちとせちゃん。布屋さんで新しい生地を見せてもらいますが、使い古した風呂敷には手放せない理由があるようで…？"
                    },
                    "Aliases": []
                },
                "15": {
                    "ID": "7009007",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7009007",
                    "Titles": {
                        "eng": "Episode15",
                        "jpn": "第15羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "北野天満宮にやってきたちとせちゃん。\r\n神社の神様として祀られている菅原道真公について教えてもらうのですが、その途中で目線の先に人影が見えて…？"
                    },
                    "Aliases": []
                },
                "16": {
                    "ID": "7009008",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7009008",
                    "Titles": {
                        "eng": "Episode16",
                        "jpn": "第16羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "朝の道で、通学中の小学生たちと会ったちとせちゃん。どうやら1人、列に遅れて歩いている男の子がいるようです。するとなぜか、ちとせちゃんは毎日同じ時間に現れて…？"
                    },
                    "Aliases": []
                },
                "17": {
                    "ID": "7020506",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7020506",
                    "Titles": {
                        "eng": "Episode17",
                        "jpn": "第17羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "お腹を鳴らして夜の京都を歩くちとせちゃん。サラリーマンの男性がご飯に誘ってくれました。料理にがっつくちとせちゃんの一方、彼はお酒を片手に愚痴モードのようで…？"
                    },
                    "Aliases": []
                },
                "18": {
                    "ID": "7031443",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7031443",
                    "Titles": {
                        "eng": "Episode18",
                        "jpn": "第18羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "軒先で雨宿りをするちとせちゃんの元に、お店の女性が提灯を灯しにやってきます。その提灯に見惚れたちとせちゃんは、鳥絵柄のものがあると聞いて探しに行きますが…？"
                    },
                    "Aliases": []
                },
                "19": {
                    "ID": "7079897",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7079897",
                    "Titles": {
                        "eng": "Episode19",
                        "jpn": "第19羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "アイスきゅうりを食べに、お友達の千昭と三千院に行くことになったちとせちゃん。そんな中、ちとせちゃんはなぜかおじいさんとおばあさんに名前を間違えられて…？"
                    },
                    "Aliases": []
                },
                "20": {
                    "ID": "7079898",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7079898",
                    "Titles": {
                        "eng": "Episode20",
                        "jpn": "第20羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "貴船の川床にやってきたちとせちゃん。お店は予約でいっぱいのため惜しくも退散。その足で貴船神社へと向かいます。するとそこで七夕の笹飾りを見つけて…？"
                    },
                    "Aliases": []
                },
                "21": {
                    "ID": "7079899",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7079899",
                    "Titles": {
                        "eng": "Episode21",
                        "jpn": "第21羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "五山の送り火が行われる日、「昔見た大文字の場所を知りたい」と言うおばあさんに出会ったちとせちゃん。一緒に探しに行きますが、なかなかその場所は見つからなくて…。"
                    },
                    "Aliases": []
                },
                "22": {
                    "ID": "7079901",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7079901",
                    "Titles": {
                        "eng": "Episode22",
                        "jpn": "第22羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "お友達の橘家で涼むちとせちゃん。横では自由研究を出せていない千昭が姉の千尋に笑われています。しかしそこへ、鞍馬山にある生物が現れたという噂が飛び込んで…？"
                    },
                    "Aliases": []
                },
                "23": {
                    "ID": "7094291",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7094291",
                    "Titles": {
                        "eng": "Episode23",
                        "jpn": "第23羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "鞍馬山で「雪之助」らしき物影に出会ったちとせちゃんと千昭。逃げて行くその姿を走って追いかけますが、その勢いで思わずちとせちゃんが階段から転げ落ちてしまい…？"
                    },
                    "Aliases": []
                },
                "24": {
                    "ID": "7094293",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/okoshiyasu-chitosechan/episodes/7094293",
                    "Titles": {
                        "eng": "Episode24",
                        "jpn": "第24羽"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "夕暮れ時、人々は自分のお家へと帰ります。どうやらちとせちゃんも、あるところへ向かっているようですが…。一体ちとせちゃんはどこへ帰っているのでしょうか？"
                    },
                    "Aliases": []
                }
            }
        }
    }
}