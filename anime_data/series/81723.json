            "# Episodes": 49,
            "Episodes": {
                "1": {
                    "ID": "360705",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360705",
                    "Titles": {
                        "eng": "Super Man Racing Across The Heavens",
                        "jpn": "天駆ける超人"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "空から落ちてきた一筋の光。現場に駆け付けたスペースナイツのノアルとアキは、そこで瀕死の重傷を負った青年に出会う。記憶を失ったという彼を、ノアルは「Ｄボウイ」と名付ける。"
                    },
                    "Aliases": []
                },
                "2": {
                    "ID": "360706",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360706",
                    "Titles": {
                        "eng": "The Lonely Warrior",
                        "jpn": "孤独の戦士"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "テッカマンブレードに変身したＤボウイにノアルが詰め寄る。だが、Ｄボウイはノアルの疑問に答える事なく、軌道衛星基地オービタルリングに向かう。そこで彼等が見た物は・・・。"
                    },
                    "Aliases": []
                },
                "3": {
                    "ID": "360707",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360707",
                    "Titles": {
                        "eng": "The Allied Defense Force's Ambition",
                        "jpn": "防衛軍の野望"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "テッカマンブレードの活躍を知った連合防衛軍のコルベット准将は、フリーマンにＤボウイの引き渡しを要求する。だが、フリーマンはこれを拒否する。その時、Ｄボウイは・・・。"
                    },
                    "Aliases": []
                },
                "4": {
                    "ID": "360708",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360708",
                    "Titles": {
                        "eng": "Deserter Without A Cause",
                        "jpn": "理由なき敵前逃亡"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "スペースナイツの一員となったＤボウイだったが心は頑ななままだった。そんな彼の心を開かそうと努力するアキ。依然、Ｄボウイを危険視するノアル。それぞれの思いが交錯する中、ラダムとの戦いが・・・。"
                    },
                    "Aliases": []
                },
                "5": {
                    "ID": "360709",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360709",
                    "Titles": {
                        "eng": "Kill Me!",
                        "jpn": "オレを殺せ"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "戦いのさなか、敵前逃亡したブレードによりアキは重傷を負ってしまう。追及の声が上がるなかＤボウイは悲痛な告白をする。自分がテッカマンでいられる時間は３０分だと・・・。"
                    },
                    "Aliases": []
                },
                "6": {
                    "ID": "360710",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360710",
                    "Titles": {
                        "eng": "Tek-Set Malfunctions!",
                        "jpn": "テックセット不能"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "テッカマンダガーの罠に陥り、変身不能となったＤボウイ。最大の危機は脱したもの、変身に必要なクリスタルを破損してしまう。戦うすべをなくした戦士の叫びが寂寞たる宇宙にこだまする。"
                    },
                    "Aliases": []
                },
                "7": {
                    "ID": "360711",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360711",
                    "Titles": {
                        "eng": "Launch Of The Mobile Unit Pegas",
                        "jpn": "機動兵ペガス発進"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイの危機はなお続く。テックセットできないＤボウイはテッカマンダガーに追い詰められていく。その絶体絶命の危機を救ったのは、鋼鉄の救世主だった。"
                    },
                    "Aliases": []
                },
                "8": {
                    "ID": "360712",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360712",
                    "Titles": {
                        "eng": "The Mysterious War Correspondent",
                        "jpn": "謎の従軍記者"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "連合宇宙軍から派遣された従軍カメラマン、バルザック・アシモフ。放埒な従軍記者は執拗なまでにＤボウイの秘密を探る。作戦に取材同行したその先で、彼の背後に蠢くもう一つの影を見る・・・。"
                    },
                    "Aliases": []
                },
                "9": {
                    "ID": "360713",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360713",
                    "Titles": {
                        "eng": "Save the Jupiter Crew!",
                        "jpn": "救出！木星クルー"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ラダムに蹂躙される以前に旅立った一団が、勇敢にも地球へ帰還しようとしていた。その、輸送船団を指揮するカル博士。命を懸けたカルの行動に、Ｄボウイの見たものは・・・。"
                    },
                    "Aliases": []
                },
                "10": {
                    "ID": "360714",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360714",
                    "Titles": {
                        "eng": "Lullaby Echoing In The Battlefield",
                        "jpn": "戦火に響く子守歌"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "特殊部隊はオービタルリングに残る宇宙艇を奪還しようとするが、ラダム獣と交戦する結果となる。救助に向ったＤボウイの説得を無視し、バーナード軍曹はオービタルリングを目指す。彼の言う「やるべき事」とは・・・。"
                    },
                    "Aliases": []
                },
                "11": {
                    "ID": "360715",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360715",
                    "Titles": {
                        "eng": "The D-Boy File",
                        "jpn": "Ｄボウイファイル"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "連合宇宙暦１９２年。全ての流れが変わろうとしていたあの日、人類の前に彼は現れた。青年の名はＤボウイ。彼についての極秘ファイルが今明かされる。"
                    },
                    "Aliases": []
                },
                "12": {
                    "ID": "360716",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360716",
                    "Titles": {
                        "eng": "The Red Menace, Tekkaman Evil",
                        "jpn": "赤い戦慄エビル"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ラダムの幼獣を倒す、防衛軍とスペースナイツの共同作戦、オペレーションサンセットは大成功のうちに終了した。だが、勝利に沸く彼等の前に絶望という赤い閃光が走り抜ける。"
                    },
                    "Aliases": []
                },
                "13": {
                    "ID": "360717",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360717",
                    "Titles": {
                        "eng": "Twin Brothers Destined To Fight",
                        "jpn": "宿命の兄弟"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "裏切り者を始末するため、兄を追い求めて地上に降りたシンヤことテッカマンエビル、宿命の名のもとに、Ｄボウイとシンヤは引かれ合うように運命の再会をする。そのシンヤの瞳に昔の面影はなかった・・・。"
                    },
                    "Aliases": []
                },
                "14": {
                    "ID": "360718",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360718",
                    "Titles": {
                        "eng": "The Demon Is My Brother",
                        "jpn": "血をわけた悪魔"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "スペースナイツはラダム獣に襲撃された発電所を救うために出動する。だが、それはエビルがブレードに仕向けた罠であった。思い出の地。汚れなき過去。翻弄されたブレードはついに・・・。"
                    },
                    "Aliases": []
                },
                "15": {
                    "ID": "360719",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360719",
                    "Titles": {
                        "eng": "The Evil Spirit Revives",
                        "jpn": "魔神蘇る"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "変身リミットを越えてしまったブレード。悪辣な心に支配されたＤボウイに仲間の叫びは聞こえない。その危機を救ったのは清婉な少女、ミリーの姿であった。"
                    },
                    "Aliases": []
                },
                "16": {
                    "ID": "360720",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360720",
                    "Titles": {
                        "eng": "Portrait Of Betrayal",
                        "jpn": "裏切りの肖像"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ふたつの顔を持つ男、バルザック・アシモフ。もう一人の彼が暗躍を始める。そして、テッカマンの解析データを盗むべく、潜入したその先で見たものとは・・・。"
                    },
                    "Aliases": []
                },
                "17": {
                    "ID": "360721",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360721",
                    "Titles": {
                        "eng": "Savior Of Steel",
                        "jpn": "鋼鉄の救世主"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "自分が暴走する凶器と化すことに苦悩するＤボウイ。襲来したラダム獣に苦悩をしいられたブレードを救ったのは一筋の光弾だった。土煙りに浮かぶ影。"
                    },
                    "Aliases": []
                },
                "18": {
                    "ID": "360722",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360722",
                    "Titles": {
                        "eng": "The Price For Glory",
                        "jpn": "栄光への代償"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ブレードの危機を救ったのはソルテッカマンに搭乗したバルザックであった。その活躍が認められたバルザックとマルローは栄光の階段を登りだす。だが、その果てに悲劇は待ち受ける。"
                    },
                    "Aliases": []
                },
                "19": {
                    "ID": "360723",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360723",
                    "Titles": {
                        "eng": "Warrior With A Closed Heart",
                        "jpn": "心閉ざした戦士"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ソルテッカマンを中心にラダムに対する反抗作戦が始まるが、エビルの反撃を受け完全なる失敗に終わった。その頃、囚われのＤボウイは悪魔と戦っていた。"
                    },
                    "Aliases": []
                },
                "20": {
                    "ID": "360724",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360724",
                    "Titles": {
                        "eng": "Resurrected! Transformation Of Rage",
                        "jpn": "復活！怒りの変身"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "圧倒的なラダムの攻撃により防衛軍は崩壊寸前を迎えていた。尚もＤボウイは己の体に潜む悪魔に怯え、戦いを拒絶する。その凍りついた心を溶かしたのはアキの魂の叫びだった。"
                    },
                    "Aliases": []
                },
                "21": {
                    "ID": "360725",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360725",
                    "Titles": {
                        "eng": "Premonition Of Love And Death",
                        "jpn": "愛と死の予感"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "アキの献身的な思いが伝わり、長き闇の中から抜け出したＤボウイ。だが、新たなる力を手にいれたエビルがブレードに迫る。そしてまた一人・・・。"
                    },
                    "Aliases": []
                },
                "22": {
                    "ID": "360726",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360726",
                    "Titles": {
                        "eng": "Miyuki's Decision",
                        "jpn": "ミユキの決意"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "兄、タカヤを求めて、ミユキは一人砂漠に彷徨う。エビルのＰＳＹボルテッカに対抗すべく、ペガスのパワーアップが続けられる中、エビルの猛攻は続く。そして・・・。"
                    },
                    "Aliases": []
                },
                "23": {
                    "ID": "360727",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360727",
                    "Titles": {
                        "eng": "Reunion Of Scarred Souls",
                        "jpn": "傷だらけの再会"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "完成したハイコートボルテッカは見事、宿敵エビルのＰＳＹボルテッカを打ち砕く。だが、Ｄボウイと再会したレイピアは傷付き生死を彷徨う。そんな中、Ｄボウイの口から過去なき者の告白が・・・。"
                    },
                    "Aliases": []
                },
                "24": {
                    "ID": "360728",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360728",
                    "Titles": {
                        "eng": "Dark Past Revealed",
                        "jpn": "引き裂かれた過去"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイはアキの心に触れて己の過去を明らかにする。ラダムに支配された肉親をその手で倒さなければならなかったその数奇な運命に、仲間は涙するしかなかった。"
                    },
                    "Aliases": []
                },
                "25": {
                    "ID": "360729",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360729",
                    "Titles": {
                        "eng": "A New Demon",
                        "jpn": "新たなる悪魔"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "防衛軍で狂気の作戦が進行する中、回復したミユキはＤボウイと思い出の地でやすらぎの時を過ごしていた。だが、ひとときの幸せも新たなる悪魔によって破られる。"
                    },
                    "Aliases": []
                },
                "26": {
                    "ID": "360730",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360730",
                    "Titles": {
                        "eng": "Battle To The Death",
                        "jpn": "死をかけた戦い"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "エビルたちの猛攻によりスペースナイツ基地は壊滅寸前を迎える。基地を防衛することの出来ないＤボウイに変わり、その絶体絶命の危機にミユキは一人、エビルたちに立ち向かう。"
                    },
                    "Aliases": []
                },
                "27": {
                    "ID": "360731",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360731",
                    "Titles": {
                        "eng": "Legacy For The Survivors",
                        "jpn": "残りし者への遺産"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "連合地球暦１９２年５月６日。スペースナイツ基地は地上から姿を消した。炎の揺らめきの中、残された一枚のファイルが輝かしい人類の未来を信じて静かに語りはじめる。"
                    },
                    "Aliases": []
                },
                "28": {
                    "ID": "360732",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360732",
                    "Titles": {
                        "eng": "The White Spirit",
                        "jpn": "白い魔人"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "スペースナイツ基地が崩壊しラダムに制圧された地上では人間によるゲリラ戦が展開されていた。そこにはソルテッカマンで戦うノアルの姿があり、アックスを前に危機に陥ったその時、忽然と空白の戦士が現れる。"
                    },
                    "Aliases": []
                },
                "29": {
                    "ID": "360733",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360733",
                    "Titles": {
                        "eng": "A Boquet On The Battlefield",
                        "jpn": "戦いの野に花束を"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "アキたちと再会したＤボウイは、月面ラダム基地へ向うために必要な敵のクリスタルを求めて旅を続けていた。途中、燃料補給に寄った町で、Ｄボウイたちが見たものは？"
                    },
                    "Aliases": []
                },
                "30": {
                    "ID": "360734",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360734",
                    "Titles": {
                        "eng": "Traces Of My Father",
                        "jpn": "父の面影"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "トレーラーでの旅は続く・・・。Ｄボウイたちはノアルの生家を訪れた。だが、故郷はラダムによって無残にも切り裂かれていた。そして人々の心までも。"
                    },
                    "Aliases": []
                },
                "31": {
                    "ID": "360735",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360735",
                    "Titles": {
                        "eng": "City Of Vengeance",
                        "jpn": "復讐の街"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "舞台は仲間の敵を討つため、ラダム獣を待ち続ける。レビンの心を揺らす夢なき者の掟。だが、制止も空しく死を懸けた戦いが今、始まる。"
                    },
                    "Aliases": []
                },
                "32": {
                    "ID": "360736",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360736",
                    "Titles": {
                        "eng": "Waiting Girl",
                        "jpn": "待ちわびた少女"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ラダム樹の森が迫る中、古城に住み続ける少女とロボット。少女は伯父の帰りを待ち続ける。琥珀色の迷宮の中でＤボウイたちが見たものは？"
                    },
                    "Aliases": []
                },
                "33": {
                    "ID": "360737",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360737",
                    "Titles": {
                        "eng": "Reunion In The Wilderness",
                        "jpn": "荒野の再会"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "アックスとの戦いが着実に迫りつつある中、Ｄボウイはラダム獣に襲われた女性、リルルを助け出し彼女の家に立ち寄る。そこで、出会った驚くべき人物に、Ｄボウイたちは・・・。"
                    },
                    "Aliases": []
                },
                "34": {
                    "ID": "360746",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360746",
                    "Titles": {
                        "eng": "Brothers Of Dark And Light",
                        "jpn": "光と影の兄弟"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "復活の時に向けて練習に余念のないシンヤの脳裏に兄、タカヤの思い出が蘇る。努力家のシンヤは天性の才能で勝利を得ていたタカヤに劣等感を抱いていた。そして、Ｄボウイも同じ過去を共有していた・・・"
                    },
                    "Aliases": []
                },
                "35": {
                    "ID": "360738",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360738",
                    "Titles": {
                        "eng": "Enemy In The Fog",
                        "jpn": "霧の中の敵"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイたちは遂にアックスと遭遇する。アックスは「ラダムに戻れ」と最後の通告をするが、Ｄボウイはそれを拒否。そして今、過去を越え、かつての師を越えるための戦いが始まる。"
                    },
                    "Aliases": []
                },
                "36": {
                    "ID": "360739",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360739",
                    "Titles": {
                        "eng": "A Decisive Battle!! Axe",
                        "jpn": "決戦！！アックス"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ブレードとアックスは巨大ラダム獣の作り出したコロシアムの中で雌雄を決する。だが、アックスの猛攻の前にボルテッカの使えないブレードは最大の危機に陥る。"
                    },
                    "Aliases": []
                },
                "37": {
                    "ID": "360740",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360740",
                    "Titles": {
                        "eng": "The Decayed Body",
                        "jpn": "蝕まれた肉体"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイは新たなるスペースナイツ基地を前にして、肉体の変調に気付きはじめていた。だが、迫り来るラダム獣を前に決死のテックセットをする。Ｄボウイは戦いに勝利はしたもの・・・。"
                    },
                    "Aliases": []
                },
                "38": {
                    "ID": "360741",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360741",
                    "Titles": {
                        "eng": "Labyrinth Of Death",
                        "jpn": "死への迷宮"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイは度重なるテックセットによって、その肉体は組織崩壊を起こしていた。生き延び、ラダムを倒すため、己の命を懸けて新たなる進化、ブラスター化を決意する。成功の確率は５０％・・・。"
                    },
                    "Aliases": []
                },
                "39": {
                    "ID": "360743",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360743",
                    "Titles": {
                        "eng": "Super Warrior Blaster",
                        "jpn": "超戦士ブラスター"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイは未知なる可能性に懸けて、生と死の狭間で独り戦う。そこに、テッカマンランスが急襲し、その危機に独眼の男が立ちはだかる。そして、Ｄボウイはついに！"
                    },
                    "Aliases": []
                },
                "40": {
                    "ID": "360745",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360745",
                    "Titles": {
                        "eng": "United in Love and Battle",
                        "jpn": "愛と戦いの二人"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイはブラスターテッカマンとして蘇った。運命の出会いから、テッカマンとして戦い続けるＤボウイの姿を見続けてきたアキの瞳に映るものとは。"
                    },
                    "Aliases": []
                },
                "41": {
                    "ID": "360747",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360747",
                    "Titles": {
                        "eng": "Evil: The Demon is Resurrected",
                        "jpn": "エビル・蘇る悪魔"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイはブラスターテッカマンとして蘇ったが、服作用による記憶の欠如という新たな病魔に冒されはじめていた。そんな中、復活のエビルがパワーアップして襲いかかる。"
                    },
                    "Aliases": []
                },
                "42": {
                    "ID": "360748",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360748",
                    "Titles": {
                        "eng": "Collision! The Red Nemesis",
                        "jpn": "激突！赤い宿敵"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "パワーアップしたエビルの攻撃もブラスターテッカマンの前には無力であった。Ｄボウイはブラスター化の代償として、記憶を失い始めていた。ミユキの好きな花さえも忘れたＤボウイは・・・。"
                    },
                    "Aliases": []
                },
                "43": {
                    "ID": "360749",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360749",
                    "Titles": {
                        "eng": "Farewell Bullets",
                        "jpn": "訣別の銃弾"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ついにオービタルリングの奪回作戦が始まり、その中にバーナードの妻、アンジェラの姿があった。記憶の欠如はバーナードの事さえも忘れてしまい、ブレードは戦いの最中、ラダムマザーの罠に捕まる。"
                    },
                    "Aliases": []
                },
                "44": {
                    "ID": "360750",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360750",
                    "Titles": {
                        "eng": "The Approaching Darkness",
                        "jpn": "迫り来る闇"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "Ｄボウイとソードは残されたスペースポートを目指す。だが、Ｄボウイはテックセットをする事さえ忘れてしまう。Ｄボウイは迫り来るソードの殺意の前に・・・。"
                    },
                    "Aliases": []
                },
                "45": {
                    "ID": "360751",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360751",
                    "Titles": {
                        "eng": "The True Invader",
                        "jpn": "真実の侵略者"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "シンヤは憧れと憎しみを胸にＤボウイと戦った。囚われのシンヤは屈辱の叫びをあげ、自らの存在の証しを求めてさらなる新化を決意する。"
                    },
                    "Aliases": []
                },
                "46": {
                    "ID": "360752",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360752",
                    "Titles": {
                        "eng": "The House in which Time Stood Still",
                        "jpn": "時の止まった家"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ラダムの来るべき日に備えて、人類をオービタルリングに移す計画が始まった。そんな中、記憶の渕を彷徨うＤボウイはアキと共に我が家に立ち寄った。そこには戻れる過去が詰まっていた・・・。"
                    },
                    "Aliases": []
                },
                "47": {
                    "ID": "360753",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360753",
                    "Titles": {
                        "eng": "Destiny of Darkness and Death",
                        "jpn": "闇と死の運命"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ブルーアース号は完成したカタパルトを使って月を目指す。テッカマンソードの急襲によりその道が閉ざされたとき、人類の未来のためバルザックはその身を懸けて立ち向かう。"
                    },
                    "Aliases": []
                },
                "48": {
                    "ID": "360754",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360754",
                    "Titles": {
                        "eng": "Heroic! The Death of Evil",
                        "jpn": "壮烈！エビル死す"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "兄を尊敬しながら越えたかった弟。それが人間であってもラダムであっても・・・。ついにブレードとエビルは雌雄を決する。そんな二人にアキの叫びは届かない。戦う為に生まれし兄弟の決戦の行く末は・・・。"
                    },
                    "Aliases": []
                },
                "49": {
                    "ID": "360755",
                    "TYPE": null,
                    "URL": "https://www.thetvdb.com/series/tekkaman-blade/episodes/360755",
                    "Titles": {
                        "eng": "A Life Ends",
                        "jpn": "燃えつきる命"
                    },
                    "Summaries": {
                        "eng": "",
                        "jpn": "ついにオメガが覚醒する。Ｄボウイは地球の未来を懸けて、ラダムに最後の戦いを挑む。忘却の果て・・・。彼に残されたものは・・・。"
                    },
                    "Aliases": []
                }
            }
        }
    }
}